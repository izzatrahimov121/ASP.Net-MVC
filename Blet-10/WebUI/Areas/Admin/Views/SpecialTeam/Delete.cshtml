@model SpecialTeam
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<div class="col-6 mt-3">
	<p></p>
	<p>Image: <img src="~/assets/img/team//@Model.Image" height="100px" /><br /><br /></p>
	<p>Name:  <h3>@Model.Fullname</h3><br /></p>
	<p>Position:  <h3>@Model.Position</h3><br /></p>
	<div class="mt-3">
		<form method="post" class="formdelete">
			<button class="btn btn-danger deleteButton">Delete</button>
		</form>
	</div>
	<a class="btn btn-info" asp-action="Index">Back</a>
</div>

